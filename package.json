{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "dev:pre": "vite --mode pre",
    "dev:pro": "vite --mode pro",
    "build": "run-p \"build-only {@}\" --",
    "build:check": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "build-only:dev": "vite build --mode dev",
    "new:page": "node ./scripts/index.mjs --ts",

    "preview": "vite preview",
    "test:unit": "vitest",
    "type-check": "vue-tsc --build --force",
    "ts:check": "vue-tsc --noEmit",

    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:script": "eslint --fix --ext .js --ext .ts --ext .vue pages/",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:prettierrc": "prettier --write .",
    "format": "prettier --write pages/"
  },
  "dependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "axios": "^1.6.8",
    "chalk": "^4.1.2",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "postcss-px-to-viewport": "^1.1.1",
    "reset-css": "^5.0.2",
    "vant": "^4.8.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "web-storage-cache": "^1.1.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.24.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.72.0",
    "sass-loader": "^14.1.1",
    "stylelint": "^16.3.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.6",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.6"
  }
}
